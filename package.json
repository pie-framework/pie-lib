{
  "name": "@pie-lib/pie-lib",
  "description": "the pie-libs mono repo",
  "private": true,
  "license": "MIT",
  "config": {
    "scope": "none"
  },
  "engines": {
    "node": "^9.10.0"
  },
  "scripts": {
    "static": "./scripts/static",
    "test": "./scripts/test",
    "dev": "./scripts/dev",
    "lint": "./node_modules/.bin/eslint --ext .js --ext .jsx packages/**/src",
    "postinstall": "cd demo && yarn install",
    "lint-two": "lerna exec -- \\$LERNA_ROOT_PATH/node_modules/.bin/eslint  --ignore-path \\$LERNA_ROOT_PATH/.eslintignore --ext .js --ext .jsx src",
    "clean": "lerna exec -- rm -fr lib",
    "release": "npm run clean && npm run build && npm test && lerna publish --conventional-commits",
    "build": "npm run clean && npm run lint-two && lerna exec -- \\$LERNA_ROOT_PATH/node_modules/.bin/babel --ignore '/__test__/','/__tests__/' src -d lib",
    "build:light": "lerna exec -- \\$LERNA_ROOT_PATH/node_modules/.bin/babel --ignore '/__test__/','/__tests__/' src -d lib"
  },
  "devDependencies": {
    "@babel/runtime": "7.2.0",
    "@pie-framework/build-helper": "^1.0.3",
    "@pie-lib/test-utils": "^0.0.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.0",
    "eslint-plugin-react": "^7.12.3",
    "fs-extra": "^7.0.1",
    "jest": "^23.6.0",
    "lerna": "^3.10.2",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "regenerator-runtime": "^0.13.1",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "yarn": "^1.13.0"
  },
  "dependencies": {
    "@material-ui/codemod": "^1.1.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
